{
    "family": "gymtinued-backend-task",
    "containerDefinitions": [
      {
        "name": "gymtinued-backend",
        "image": "docker.io/blancjh/gymtinued-backend:latest",
        "cpu": 0,
        "memory": 1024,
        "portMappings": [
          {
            "containerPort": 5000,
            "hostPort": 5000,
            "protocol": "tcp",
            "name": "gymtinued-backend-port-protocol",
            "appProtocol": "http"
          }
        ],
        "essential": true,
        "environment": [
          {
            "name": "JWT_SECRET",
            "value": "${{ secrets.JWT_SECRET }}"
          },
          {
            "name": "SPRING_DATASOURCE_URL",
            "value": "${{ secrets.SPRING_DATASOURCE_URL }}"
          },
          {
            "name": "SPRING_DATASOURCE_PASSWORD",
            "value": "${{ secrets.SPRING_DATASOURCE_PASSWORD }}"
          },
          {
            "name": "CHATGPT_API_URL",
            "value": "${{ secrets.CHATGPT_API_URL }}"
          },
          {
            "name": "CHATGPT_API_key",
            "value": "${{ secrets.CHATGPT_API_key }}"
          },
          {
            "name": "SPRING_DATASOURCE_USERNAME",
            "value": "${{ secrets.SPRING_DATASOURCE_USERNAME }}"
          },
          {
            "name": "JWT_EXPIRATION",
            "value": "${{ secrets.JWT_EXPIRATION }}"
          }
        ],
        "environmentFiles": [],
        "mountPoints": [],
        "volumesFrom": [],
        "ulimits": [],
        "systemControls": []
      }
    ],
    "taskRoleArn": "arn:aws:iam::985539781323:role/ECS_RDS_FULL",
    "executionRoleArn": "arn:aws:iam::985539781323:role/ECS_RDS_FULL",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
    },
    "enableFaultInjection": false
  }
